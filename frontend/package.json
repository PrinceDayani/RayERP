{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "validate-env": "node validate-env.js",
    "dev": "next dev --turbo",
    "dev:validate": "npm run validate-env && next dev --turbo",
    "dev:fast": "next dev --turbo --experimental-https",
    "build": "npm run clean && next build",
    "build:fast": "next build --experimental-build-mode=compile",
    "build:analyze": "ANALYZE=true npm run validate-env && next build",
    "build:prod": "NODE_ENV=production npm run validate-env && npm run clean && next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint --fix --max-warnings 0",
    "lint:check": "next lint",
    "type-check": "tsc --noEmit --incremental --skipLibCheck",
    "type-check:fast": "tsc --noEmit --skipLibCheck",
    "clean": "if exist .next rmdir /s /q .next && if exist out rmdir /s /q out && if exist node_modules\\.cache rmdir /s /q node_modules\\.cache && if exist tsconfig.tsbuildinfo del tsconfig.tsbuildinfo",
    "clean:all": "npm run clean && if exist node_modules rmdir /s /q node_modules && npm install",
    "export": "next export",
    "serve": "npx serve out",
    "docker:build": "docker build -t rayerp-frontend .",
    "docker:run": "docker run -p 3000:3000 rayerp-frontend",
    "preinstall": "npx only-allow npm"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@types/papaparse": "^5.3.15",
    "@types/react-window": "^1.8.8",
    "axios": "^1.12.0",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.12",
    "html2canvas": "^1.4.1",
    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.476.0",
    "next": "^16.0.3",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.2",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-colorful": "^5.6.1",
    "react-day-picker": "^9.4.3",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.5.2",
    "react-window": "^2.2.3",
    "recharts": "^2.15.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.1.7",
    "@types/file-saver": "^2.0.7",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.19.25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
